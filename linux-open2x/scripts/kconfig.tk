proc menu36 {w title} {
	set oldFocus [focus]
	catch {focus .menu30}
	catch {destroy $w; unregister_active 36}
	toplevel $w -class Dialog
	wm withdraw $w
	global active_menus
	set active_menus [lsort -integer [linsert $active_menus end 36]]
	message $w.m -width 400 -aspect 300 -text \
		"Wan interfaces"  -relief raised
	pack $w.m -pady 10 -side top -padx 10
	wm title $w "Wan interfaces" 

	bind $w <Escape> "catch {focus $oldFocus}; destroy $w; unregister_active 36; break"
	set nextscript "catch {focus $oldFocus}; destroy $w; unregister_active 36; menu37 .menu37 \"$title\""
	frame $w.f
	button $w.f.back -text "OK" \
		-width 15 -command "catch {focus $oldFocus}; destroy $w; unregister_active 36"
	button $w.f.next -text "Next" -underline 0\
		-width 15 -command $nextscript
	bind all <Alt-n> $nextscript
	button $w.f.prev -text "Prev" -underline 0\
		-width 15 -command "catch {focus $oldFocus}; destroy $w; unregister_active 36; menu35 .menu35 \"$title\""
	bind $w <Alt-p> "catch {focus $oldFocus}; destroy $w; unregister_active 36; menu35 .menu35 \"$title\";break"
	pack $w.f.back $w.f.next $w.f.prev -side left -expand on
	pack $w.f -pady 10 -side bottom -anchor w -fill x
	frame $w.topline -relief ridge -borderwidth 2 -height 2
	pack $w.topline -side top -fill x

	frame $w.botline -relief ridge -borderwidth 2 -height 2
	pack $w.botline -side bottom -fill x

	frame $w.config
	pack $w.config -fill y -expand on

	scrollbar $w.config.vscroll -command "$w.config.canvas yview"
	pack $w.config.vscroll -side right -fill y

	canvas $w.config.canvas -height 1\
		-relief flat -borderwidth 0 -yscrollcommand "$w.config.vscroll set" \
		-width [expr [winfo screenwidth .] * 1 / 2] 
	frame $w.config.f
	bind $w <Key-Down> "$w.config.canvas yview scroll  1 unit;break;"
	bind $w <Key-Up> "$w.config.canvas yview scroll  -1 unit;break;"
	bind $w <Key-Next> "$w.config.canvas yview scroll  1 page;break;"
	bind $w <Key-Prior> "$w.config.canvas yview scroll  -1 page;break;"
	bind $w <Key-Home> "$w.config.canvas yview moveto 0;break;"
	bind $w <Key-End> "$w.config.canvas yview moveto 1 ;break;"
	pack $w.config.canvas -side right -fill y


	bool $w.config.f 36 0 "Wan interfaces support" CONFIG_WAN
	dep_tristate $w.config.f 36 1 "  Comtrol Hostess SV-11 support" CONFIG_HOSTESS_SV11
	dep_tristate $w.config.f 36 2 "  COSA/SRP sync serial boards support" CONFIG_COSA
	bool $w.config.f 36 3 "  MultiGate (COMX) synchronous serial boards support" CONFIG_COMX
	dep_tristate $w.config.f 36 4 "    Support for COMX/CMX/HiCOMX boards" CONFIG_COMX_HW_COMX
	dep_tristate $w.config.f 36 5 "    Support for LoCOMX board" CONFIG_COMX_HW_LOCOMX
	dep_tristate $w.config.f 36 6 "    Support for MixCOM board" CONFIG_COMX_HW_MIXCOM
	dep_tristate $w.config.f 36 7 "    Support for MUNICH based boards: SliceCOM, PCICOM (WelCOM)" CONFIG_COMX_HW_MUNICH
	dep_tristate $w.config.f 36 8 "    Support for HDLC and syncPPP protocols on MultiGate boards" CONFIG_COMX_PROTO_PPP
	dep_tristate $w.config.f 36 9 "    Support for LAPB protocol on MultiGate boards" CONFIG_COMX_PROTO_LAPB
	dep_tristate $w.config.f 36 10 "    Support for LAPB protocol on MultiGate boards" CONFIG_COMX_PROTO_LAPB
	dep_tristate $w.config.f 36 11 "    Support for Frame Relay on MultiGate boards" CONFIG_COMX_PROTO_FR
	dep_tristate $w.config.f 36 12 "  DSCC4 support" CONFIG_DSCC4
	bool $w.config.f 36 13 "    Etinc PCISYNC features" CONFIG_DSCC4_PCISYNC
	bool $w.config.f 36 14 "    GPIO and PCI #RST pins wired together" CONFIG_DSCC4_PCI_RST
	tristate $w.config.f 36 15 "  LanMedia Corp. SSI/V.35, T1/E1, HSSI, T3 boards" CONFIG_LANMEDIA
	tristate $w.config.f 36 16 "  Aurora Technology, Inc. synchronous asynchronous PCI cards V2" CONFIG_ATI_XX20
	dep_tristate $w.config.f 36 17 "  Sealevel Systems 4021 support" CONFIG_SEALEVEL_4021
	tristate $w.config.f 36 18 "  SyncLink HDLC/SYNCPPP support" CONFIG_SYNCLINK_SYNCPPP
	tristate $w.config.f 36 19 "  Generic HDLC layer" CONFIG_HDLC
	bool $w.config.f 36 20 "    Raw HDLC support" CONFIG_HDLC_RAW
	bool $w.config.f 36 21 "    Raw HDLC Ethernet device support" CONFIG_HDLC_RAW_ETH
	bool $w.config.f 36 22 "    Cisco HDLC support" CONFIG_HDLC_CISCO
	bool $w.config.f 36 23 "    Frame Relay support" CONFIG_HDLC_FR
	bool $w.config.f 36 24 "    Synchronous Point-to-Point Protocol (PPP) support" CONFIG_HDLC_PPP
	bool $w.config.f 36 25 "    X.25 protocol support" CONFIG_HDLC_X25
	comment $w.config.f 36 26 "    X.25/LAPB support is disabled"
	dep_tristate $w.config.f 36 27 "    Goramo PCI200SYN support" CONFIG_PCI200SYN
	dep_tristate $w.config.f 36 28 "    Cyclades-PC300 support (RS-232/V.35, X.21, T1/E1 boards)" CONFIG_PC300
	bool $w.config.f 36 29 "      Cyclades-PC300 MLPPP support" CONFIG_PC300_MLPPP
	comment $w.config.f 36 30 "     Cyclades-PC300 MLPPP support is disabled."
	comment $w.config.f 36 31 "     Refer to the file README.mlppp, provided by PC300 package."
	dep_tristate $w.config.f 36 32 "    FarSync T-Series support" CONFIG_FARSYNC
	dep_tristate $w.config.f 36 33 "    SDL RISCom/N2 support" CONFIG_N2
	dep_tristate $w.config.f 36 34 "    Moxa C101 support" CONFIG_C101
	tristate $w.config.f 36 35 "  Frame relay DLCI support" CONFIG_DLCI
	int $w.config.f 36 36 "    Max open DLCI" CONFIG_DLCI_COUNT
	int $w.config.f 36 37 "    Max DLCI per device" CONFIG_DLCI_MAX
	dep_tristate $w.config.f 36 38 "    SDLA (Sangoma S502/S508) support" CONFIG_SDLA
	bool $w.config.f 36 39 "  WAN router drivers" CONFIG_WAN_ROUTER_DRIVERS
	dep_tristate $w.config.f 36 40 "    Sangoma WANPIPE(tm) multiprotocol cards" CONFIG_VENDOR_SANGOMA
	bool $w.config.f 36 41 "      WANPIPE Cisco HDLC support" CONFIG_WANPIPE_CHDLC
	bool $w.config.f 36 42 "      WANPIPE Frame Relay support" CONFIG_WANPIPE_FR
	bool $w.config.f 36 43 "      WANPIPE X.25 support" CONFIG_WANPIPE_X25
	bool $w.config.f 36 44 "      WANPIPE PPP support" CONFIG_WANPIPE_PPP
	bool $w.config.f 36 45 "      WANPIPE Multi-Port PPP support" CONFIG_WANPIPE_MULTPPP
	dep_tristate $w.config.f 36 46 "    Cyclom 2X(tm) cards (EXPERIMENTAL)" CONFIG_CYCLADES_SYNC
	bool $w.config.f 36 47 "      Cyclom 2X X.25 support (EXPERIMENTAL)" CONFIG_CYCLOMX_X25
	dep_tristate $w.config.f 36 48 "  LAPB over Ethernet driver (EXPERIMENTAL)" CONFIG_LAPBETHER
	dep_tristate $w.config.f 36 49 "  X.25 async driver (EXPERIMENTAL)" CONFIG_X25_ASY



	focus $w
	update_active
	global winx; global winy
	if {[winfo exists .menu30] == 0} then {menu30 .menu30 "Network device support"}
	set winx [expr [winfo x .menu30]+30]; set winy [expr [winfo y .menu30]+30]
	if {[winfo exists $w]} then {wm geometry $w +$winx+$winy}
	update idletasks
	if {[winfo exists $w]} then  {$w.config.canvas create window 0 0 -anchor nw -window $w.config.f

	$w.config.canvas configure \
		-width [expr [winfo reqwidth $w.config.f] + 1]\
		-scrollregion "-1 -1 [expr [winfo reqwidth $w.config.f] + 1] \
			 [expr [winfo reqheight $w.config.f] + 1]"

	set winy [expr [winfo reqh $w] - [winfo reqh $w.config.canvas]]
	set scry [expr [winfo screenh $w] / 2]
	set maxy [expr [winfo screenh $w] * 3 / 4]
	set canvtotal [expr [winfo reqh $w.config.f] + 2]
	if [expr $winy + $canvtotal < $maxy] {
		$w.config.canvas configure -height $canvtotal
	} else {
		$w.config.canvas configure -height [expr $scry - $winy]
		}
	}
