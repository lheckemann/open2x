menu_option menu68 68 "File systems"
proc menu68 {w title} {
	set oldFocus [focus]
	catch {destroy $w; unregister_active 68}
	toplevel $w -class Dialog
	wm withdraw $w
	global active_menus
	set active_menus [lsort -integer [linsert $active_menus end 68]]
	message $w.m -width 400 -aspect 300 -text \
		"File systems"  -relief raised
	pack $w.m -pady 10 -side top -padx 10
	wm title $w "File systems" 

	bind $w <Escape> "catch {focus $oldFocus}; destroy $w; unregister_active 68; break"
	set nextscript "catch {focus $oldFocus}; menu69 .menu69 \"$title\""
	frame $w.f
	button $w.f.back -text "Main Menu" \
		-width 15 -command "catch {focus $oldFocus}; destroy $w; unregister_active 68"
	button $w.f.next -text "Next" -underline 0\
		-width 15 -command $nextscript
	bind all <Alt-n> $nextscript
	button $w.f.prev -text "Prev" -underline 0\
		-width 15 -command "catch {focus $oldFocus}; destroy $w; unregister_active 68; menu67 .menu67 \"$title\""
	bind $w <Alt-p> "catch {focus $oldFocus}; destroy $w; unregister_active 68; menu67 .menu67 \"$title\";break"
	pack $w.f.back $w.f.next $w.f.prev -side left -expand on
	pack $w.f -pady 10 -side bottom -anchor w -fill x
	frame $w.topline -relief ridge -borderwidth 2 -height 2
	pack $w.topline -side top -fill x

	frame $w.botline -relief ridge -borderwidth 2 -height 2
	pack $w.botline -side bottom -fill x

	frame $w.config
	pack $w.config -fill y -expand on

	scrollbar $w.config.vscroll -command "$w.config.canvas yview"
	pack $w.config.vscroll -side right -fill y

	canvas $w.config.canvas -height 1\
		-relief flat -borderwidth 0 -yscrollcommand "$w.config.vscroll set" \
		-width [expr [winfo screenwidth .] * 1 / 2] 
	frame $w.config.f
	bind $w <Key-Down> "$w.config.canvas yview scroll  1 unit;break;"
	bind $w <Key-Up> "$w.config.canvas yview scroll  -1 unit;break;"
	bind $w <Key-Next> "$w.config.canvas yview scroll  1 page;break;"
	bind $w <Key-Prior> "$w.config.canvas yview scroll  -1 page;break;"
	bind $w <Key-Home> "$w.config.canvas yview moveto 0;break;"
	bind $w <Key-End> "$w.config.canvas yview moveto 1 ;break;"
	pack $w.config.canvas -side right -fill y


	bool $w.config.f 68 0 "Quota support" CONFIG_QUOTA
	dep_tristate $w.config.f 68 1 "  VFS v0 quota format support" CONFIG_QFMT_V2
	tristate $w.config.f 68 2 "Kernel automounter support" CONFIG_AUTOFS_FS
	tristate $w.config.f 68 3 "Kernel automounter version 4 support (also supports v3)" CONFIG_AUTOFS4_FS
	tristate $w.config.f 68 4 "Reiserfs support" CONFIG_REISERFS_FS
	dep_bool $w.config.f 68 5 "  Enable reiserfs debug mode" CONFIG_REISERFS_CHECK
	dep_bool $w.config.f 68 6 "  Stats in /proc/fs/reiserfs" CONFIG_REISERFS_PROC_INFO
	dep_tristate $w.config.f 68 7 "ADFS file system support (EXPERIMENTAL)" CONFIG_ADFS_FS
	dep_bool $w.config.f 68 8 "  ADFS write support (DANGEROUS)" CONFIG_ADFS_FS_RW
	dep_tristate $w.config.f 68 9 "Amiga FFS file system support (EXPERIMENTAL)" CONFIG_AFFS_FS
	dep_tristate $w.config.f 68 10 "Apple HFS file system support (EXPERIMENTAL)" CONFIG_HFS_FS
	dep_tristate $w.config.f 68 11 "Apple HFS+ (Extended HFS) file system support (EXPERIMENTAL)" CONFIG_HFSPLUS_FS
	dep_tristate $w.config.f 68 12 "BeOS file systemv(BeFS) support (read only) (EXPERIMENTAL)" CONFIG_BEFS_FS
	dep_bool $w.config.f 68 13 "  Debug Befs" CONFIG_BEFS_DEBUG
	dep_tristate $w.config.f 68 14 "BFS file system support (EXPERIMENTAL)" CONFIG_BFS_FS
	tristate $w.config.f 68 15 "Ext3 journalling file system support" CONFIG_EXT3_FS
	dep_bool $w.config.f 68 16 "  JBD (ext3) debugging support" CONFIG_JBD_DEBUG
	tristate $w.config.f 68 17 "DOS FAT fs support" CONFIG_FAT_FS
	dep_tristate $w.config.f 68 18 "  MSDOS fs support" CONFIG_MSDOS_FS
	dep_tristate $w.config.f 68 19 "    UMSDOS: Unix-like file system on top of standard MSDOS fs" CONFIG_UMSDOS_FS
	dep_tristate $w.config.f 68 20 "  VFAT (Windows-95) fs support" CONFIG_VFAT_FS
	dep_tristate $w.config.f 68 21 "EFS file system support (read only) (EXPERIMENTAL)" CONFIG_EFS_FS
	dep_tristate $w.config.f 68 22 "Journalling Flash File System (JFFS) support" CONFIG_JFFS_FS
	int $w.config.f 68 23 "JFFS debugging verbosity (0 = quiet, 3 = noisy)" CONFIG_JFFS_FS_VERBOSE
	bool $w.config.f 68 24 "JFFS stats available in /proc filesystem" CONFIG_JFFS_PROC_FS
	dep_tristate $w.config.f 68 25 "Journalling Flash File System v2 (JFFS2) support" CONFIG_JFFS2_FS
	int $w.config.f 68 26 "JFFS2 debugging verbosity (0 = quiet, 2 = noisy)" CONFIG_JFFS2_FS_DEBUG
	bool $w.config.f 68 27 "JFFS2 support for NAND chips" CONFIG_JFFS2_FS_NAND
	tristate $w.config.f 68 28 "Compressed ROM file system support" CONFIG_CRAMFS
	bool $w.config.f 68 29 "Virtual memory file system support (former shm fs)" CONFIG_TMPFS
	tristate $w.config.f 68 30 "Yet Another Flash File System (YAFFS) support" CONFIG_YAFFS
	tristate $w.config.f 68 31 "ISO 9660 CDROM file system support" CONFIG_ISO9660_FS
	dep_bool $w.config.f 68 32 "  Microsoft Joliet CDROM extensions" CONFIG_JOLIET
	dep_bool $w.config.f 68 33 "  Transparent decompression extension" CONFIG_ZISOFS
	tristate $w.config.f 68 34 "JFS filesystem support" CONFIG_JFS_FS
	dep_bool $w.config.f 68 35 "  JFS debugging" CONFIG_JFS_DEBUG
	dep_bool $w.config.f 68 36 "  JFS statistics" CONFIG_JFS_STATISTICS
	tristate $w.config.f 68 37 "Minix fs support" CONFIG_MINIX_FS
	tristate $w.config.f 68 38 "FreeVxFS file system support (VERITAS VxFS(TM) compatible)" CONFIG_VXFS_FS
	tristate $w.config.f 68 39 "NTFS file system support (read only)" CONFIG_NTFS_FS
	dep_bool $w.config.f 68 40 "  NTFS write support (DANGEROUS)" CONFIG_NTFS_RW
	tristate $w.config.f 68 41 "OS/2 HPFS file system support" CONFIG_HPFS_FS
	bool $w.config.f 68 42 "/proc file system support" CONFIG_PROC_FS
	dep_bool $w.config.f 68 43 "/dev file system support (EXPERIMENTAL)" CONFIG_DEVFS_FS
	dep_bool $w.config.f 68 44 "  Automatically mount at boot" CONFIG_DEVFS_MOUNT
	dep_bool $w.config.f 68 45 "  Debug devfs" CONFIG_DEVFS_DEBUG
	dep_bool $w.config.f 68 46 "/dev/pts file system for Unix98 PTYs" CONFIG_DEVPTS_FS
	tristate $w.config.f 68 47 "QNX4 file system support (read only)" CONFIG_QNX4FS_FS
	dep_bool $w.config.f 68 48 "  QNX4FS write support (DANGEROUS)" CONFIG_QNX4FS_RW
	tristate $w.config.f 68 49 "ROM file system support" CONFIG_ROMFS_FS
	tristate $w.config.f 68 50 "Second extended fs support" CONFIG_EXT2_FS
	tristate $w.config.f 68 51 "System V/Xenix/V7/Coherent file system support" CONFIG_SYSV_FS
	tristate $w.config.f 68 52 "UDF file system support (read only)" CONFIG_UDF_FS
	dep_bool $w.config.f 68 53 "  UDF write support (DANGEROUS)" CONFIG_UDF_RW
	tristate $w.config.f 68 54 "UFS file system support (read only)" CONFIG_UFS_FS
	dep_bool $w.config.f 68 55 "  UFS file system write support (DANGEROUS)" CONFIG_UFS_FS_WRITE
	tristate $w.config.f 68 56 "XFS filesystem support" CONFIG_XFS_FS
	dep_bool $w.config.f 68 57 "  Quota support" CONFIG_XFS_QUOTA
	dep_bool $w.config.f 68 58 "  Realtime support (EXPERIMENTAL)" CONFIG_XFS_RT
	dep_bool $w.config.f 68 59 "  Tracing support (EXPERIMENTAL)" CONFIG_XFS_TRACE
	dep_bool $w.config.f 68 60 "  Debugging support (EXPERIMENTAL)" CONFIG_XFS_DEBUG
	submenu $w.config.f 68 61 "Network File Systems" 69
	submenu $w.config.f 68 62 "Partition Types" 70
	submenu $w.config.f 68 63 "Native Language Support" 71



	focus $w
	update_active
	global winx; global winy
	set winx [expr [winfo x .]+30]; set winy [expr [winfo y .]+30]
	if {[winfo exists $w]} then {wm geometry $w +$winx+$winy}
	update idletasks
	if {[winfo exists $w]} then  {$w.config.canvas create window 0 0 -anchor nw -window $w.config.f

	$w.config.canvas configure \
		-width [expr [winfo reqwidth $w.config.f] + 1]\
		-scrollregion "-1 -1 [expr [winfo reqwidth $w.config.f] + 1] \
			 [expr [winfo reqheight $w.config.f] + 1]"

	set winy [expr [winfo reqh $w] - [winfo reqh $w.config.canvas]]
	set scry [expr [winfo screenh $w] / 2]
	set maxy [expr [winfo screenh $w] * 3 / 4]
	set canvtotal [expr [winfo reqh $w.config.f] + 2]
	if [expr $winy + $canvtotal < $maxy] {
		$w.config.canvas configure -height $canvtotal
	} else {
		$w.config.canvas configure -height [expr $scry - $winy]
		}
	}
	update idletasks
	if {[winfo exists $w]} then {
	wm maxsize $w [winfo width $w] [winfo screenheight $w]
	wm minsize $w [winfo width $w] 100

	wm deiconify $w
}
}

