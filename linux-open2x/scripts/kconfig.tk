	dep_tristate $w.config.f 81 7 "HCI DTL1 (PC Card) driver" CONFIG_BLUEZ_HCIDTL1
	dep_tristate $w.config.f 81 8 "HCI BT3C (PC Card) driver" CONFIG_BLUEZ_HCIBT3C
	dep_tristate $w.config.f 81 9 "HCI BlueCard (PC Card) driver" CONFIG_BLUEZ_HCIBLUECARD
	dep_tristate $w.config.f 81 10 "HCI UART (PC Card) driver" CONFIG_BLUEZ_HCIBTUART
	dep_tristate $w.config.f 81 11 "HCI VHCI (Virtual HCI device) driver" CONFIG_BLUEZ_HCIVHCI



	focus $w
	update_active
	global winx; global winy
