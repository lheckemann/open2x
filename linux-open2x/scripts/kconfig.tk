	global CONFIG_MCP
	global CONFIG_ARCH_SA1100
	global CONFIG_MCP_SA1100
	if {($CONFIG_MCP == 1 || $CONFIG_MCP == 4) && ($CONFIG_ARCH_SA1100 == 1 || $CONFIG_ARCH_SA1100 == 4)} then {
	set tmpvar_dep [effective_dep [list $CONFIG_MCP $CONFIG_ARCH_SA1100]];set CONFIG_MCP_SA1100 [sync_bool $CONFIG_MCP_SA1100 $tmpvar_dep 0];if {$tmpvar_dep != 1} then {configure_entry .menu75.config.f.x1 disabled {y};} else {configure_entry .menu75.config.f.x1 normal {y};}; configure_entry .menu75.config.f.x1 normal {n l}} else {configure_entry .menu75.config.f.x1 disabled {y n l}}
