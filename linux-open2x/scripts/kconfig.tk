	dep_tristate $w.config.f 57 9 "  Omaha I2C interface" CONFIG_I2C_OMAHA
	dep_tristate $w.config.f 57 10 "  Frodo I2C adapter" CONFIG_I2C_FRODO
	dep_tristate $w.config.f 57 11 "  SA1100 I2C GPIO adapter" CONFIG_I2C_BIT_SA1100_GPIO
	dep_tristate $w.config.f 57 12 "NatSemi SCx200 ACCESS.bus" CONFIG_SCx200_ACB
	dep_tristate $w.config.f 57 13 "I2C PCF 8584 interfaces" CONFIG_I2C_ALGOPCF
	dep_tristate $w.config.f 57 14 "  Elektor ISA card" CONFIG_I2C_ELEKTOR
	dep_tristate $w.config.f 57 15 "Atmel AT91RM9200 I2C Two-Wire interface (TWI)" CONFIG_I2C_AT91
	dep_tristate $w.config.f 57 16 "I2C device interface" CONFIG_I2C_CHARDEV
	dep_tristate $w.config.f 57 17 "I2C /proc interface (required for hardware sensors)" CONFIG_I2C_PROC



