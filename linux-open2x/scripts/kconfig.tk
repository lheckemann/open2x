proc menu61 {w title} {
	set oldFocus [focus]
	catch {focus .menu55}
	catch {destroy $w; unregister_active 61}
	toplevel $w -class Dialog
	wm withdraw $w
	global active_menus
	set active_menus [lsort -integer [linsert $active_menus end 61]]
	message $w.m -width 400 -aspect 300 -text \
		"Watchdog Cards"  -relief raised
	pack $w.m -pady 10 -side top -padx 10
	wm title $w "Watchdog Cards" 

	bind $w <Escape> "catch {focus $oldFocus}; destroy $w; unregister_active 61; break"
	set nextscript "catch {focus $oldFocus}; destroy $w; unregister_active 61; menu62 .menu62 \"$title\""
	frame $w.f
	button $w.f.back -text "OK" \
		-width 15 -command "catch {focus $oldFocus}; destroy $w; unregister_active 61"
	button $w.f.next -text "Next" -underline 0\
		-width 15 -command $nextscript
	bind all <Alt-n> $nextscript
	button $w.f.prev -text "Prev" -underline 0\
		-width 15 -command "catch {focus $oldFocus}; destroy $w; unregister_active 61; menu60 .menu60 \"$title\""
	bind $w <Alt-p> "catch {focus $oldFocus}; destroy $w; unregister_active 61; menu60 .menu60 \"$title\";break"
	pack $w.f.back $w.f.next $w.f.prev -side left -expand on
	pack $w.f -pady 10 -side bottom -anchor w -fill x
	frame $w.topline -relief ridge -borderwidth 2 -height 2
	pack $w.topline -side top -fill x

	frame $w.botline -relief ridge -borderwidth 2 -height 2
	pack $w.botline -side bottom -fill x

	frame $w.config
	pack $w.config -fill y -expand on

	scrollbar $w.config.vscroll -command "$w.config.canvas yview"
	pack $w.config.vscroll -side right -fill y

	canvas $w.config.canvas -height 1\
		-relief flat -borderwidth 0 -yscrollcommand "$w.config.vscroll set" \
		-width [expr [winfo screenwidth .] * 1 / 2] 
	frame $w.config.f
	bind $w <Key-Down> "$w.config.canvas yview scroll  1 unit;break;"
	bind $w <Key-Up> "$w.config.canvas yview scroll  -1 unit;break;"
	bind $w <Key-Next> "$w.config.canvas yview scroll  1 page;break;"
	bind $w <Key-Prior> "$w.config.canvas yview scroll  -1 page;break;"
	bind $w <Key-Home> "$w.config.canvas yview moveto 0;break;"
	bind $w <Key-End> "$w.config.canvas yview moveto 1 ;break;"
	pack $w.config.canvas -side right -fill y


	bool $w.config.f 61 0 "Watchdog Timer Support" CONFIG_WATCHDOG
	bool $w.config.f 61 1 "  Disable watchdog shutdown on close" CONFIG_WATCHDOG_NOWAYOUT
	tristate $w.config.f 61 2 "  Acquire SBC Watchdog Timer" CONFIG_ACQUIRE_WDT
	tristate $w.config.f 61 3 "  Advantech SBC Watchdog Timer" CONFIG_ADVANTECH_WDT
	tristate $w.config.f 61 4 "  ALi M7101 PMU on ALi 1535D+ Watchdog Timer" CONFIG_ALIM1535_WDT
	tristate $w.config.f 61 5 "  ALi M7101 PMU Watchdog Timer" CONFIG_ALIM7101_WDT
	tristate $w.config.f 61 6 "  AMD \"Elan\" SC520 Watchdog Timer" CONFIG_SC520_WDT
	tristate $w.config.f 61 7 "  Berkshire Products PC Watchdog" CONFIG_PCWATCHDOG
	dep_tristate $w.config.f 61 8 "  DC21285 watchdog" CONFIG_21285_WATCHDOG
	dep_tristate $w.config.f 61 9 "  NetWinder WB83C977 watchdog" CONFIG_977_WATCHDOG
	dep_tristate $w.config.f 61 10 "  SA1100 watchdog" CONFIG_SA1100_WATCHDOG
	dep_tristate $w.config.f 61 11 "  EPXA watchdog" CONFIG_EPXA_WATCHDOG
	dep_tristate $w.config.f 61 12 "  Omaha watchdog" CONFIG_OMAHA_WATCHDOG
	dep_tristate $w.config.f 61 13 "  AT91RM9200 watchdog" CONFIG_AT91_WATCHDOG
	dep_tristate $w.config.f 61 14 "  MMSP2 watchdog" CONFIG_MMSP2_WATCHDOG
	tristate $w.config.f 61 15 "  Eurotech CPU-1220/1410 Watchdog Timer" CONFIG_EUROTECH_WDT
	tristate $w.config.f 61 16 "  IB700 SBC Watchdog Timer" CONFIG_IB700_WDT
	tristate $w.config.f 61 17 "  ICP ELectronics Wafer 5823 Watchdog" CONFIG_WAFER_WDT
	tristate $w.config.f 61 18 "  Intel i810 TCO timer / Watchdog" CONFIG_I810_TCO
	tristate $w.config.f 61 19 "  Mixcom Watchdog" CONFIG_MIXCOMWD
	tristate $w.config.f 61 20 "  SBC-60XX Watchdog Timer" CONFIG_60XX_WDT
	dep_tristate $w.config.f 61 21 "  SC1200 Watchdog Timer (EXPERIMENTAL)" CONFIG_SC1200_WDT
	tristate $w.config.f 61 22 "  NatSemi SCx200 Watchdog" CONFIG_SCx200_WDT
	tristate $w.config.f 61 23 "  Software Watchdog" CONFIG_SOFT_WATCHDOG
	tristate $w.config.f 61 24 "  W83877F (EMACS) Watchdog Timer" CONFIG_W83877F_WDT
	tristate $w.config.f 61 25 "  WDT Watchdog timer" CONFIG_WDT
	tristate $w.config.f 61 26 "  WDT PCI Watchdog timer" CONFIG_WDTPCI
	bool $w.config.f 61 27 "    WDT501 features" CONFIG_WDT_501
	bool $w.config.f 61 28 "      Fan Tachometer" CONFIG_WDT_501_FAN
	tristate $w.config.f 61 29 "  ZF MachZ Watchdog" CONFIG_MACHZ_WDT
	dep_tristate $w.config.f 61 30 "  AMD 766/768 TCO Timer/Watchdog" CONFIG_AMD7XX_TCO



	focus $w
	update_active
	global winx; global winy
	if {[winfo exists .menu55] == 0} then {menu55 .menu55 "Character devices"}
	set winx [expr [winfo x .menu55]+30]; set winy [expr [winfo y .menu55]+30]
	if {[winfo exists $w]} then {wm geometry $w +$winx+$winy}
	update idletasks
	if {[winfo exists $w]} then  {$w.config.canvas create window 0 0 -anchor nw -window $w.config.f

	$w.config.canvas configure \
		-width [expr [winfo reqwidth $w.config.f] + 1]\
		-scrollregion "-1 -1 [expr [winfo reqwidth $w.config.f] + 1] \
			 [expr [winfo reqheight $w.config.f] + 1]"

	set winy [expr [winfo reqh $w] - [winfo reqh $w.config.canvas]]
	set scry [expr [winfo screenh $w] / 2]
	set maxy [expr [winfo screenh $w] * 3 / 4]
	set canvtotal [expr [winfo reqh $w.config.f] + 2]
	if [expr $winy + $canvtotal < $maxy] {
		$w.config.canvas configure -height $canvtotal
	} else {
		$w.config.canvas configure -height [expr $scry - $winy]
		}
	}
	update idletasks
	if {[winfo exists $w]} then {
	wm maxsize $w [winfo width $w] [winfo screenheight $w]
	wm minsize $w [winfo width $w] 100

	wm deiconify $w
}
}

